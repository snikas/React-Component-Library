{
  "name": "revel-ui-snowpack",
  "version": "0.0.1",
  "description": "React Component Library (React, MUI, babel, typescript, storybook)",
  "main": "index.js",
  "scripts": {
    "develop": "babel src --out-dir public/ --watch --extensions '.tsx'",
    "prepare": "snowpack",
    "cleanStart": "rm -rf node_modules && rm -rf web_modules && npm i && npm run dev",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check-types": "tsc"
  },
  "keywords": [
    "snowpack"
  ],
  "author": "Steven Akins",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "react": "npm:@reactesm/react@^0.1.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@types/react": "^16.9.17",
    "babel-loader": "^8.0.6",
    "snowpack": "^1.0.5",
    "typescript": "^3.7.5"
  },
  "browserslist": [
    "not IE 11"
  ]
}
